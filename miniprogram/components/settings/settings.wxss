.wx-authorization-modal {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 99998;
}

.wx-authorization-modal view {
  box-sizing: border-box;
}

.wx-authorization-modal .image {
  width: 100%;
  height: 100%;
  border-radius: inherit;
}

